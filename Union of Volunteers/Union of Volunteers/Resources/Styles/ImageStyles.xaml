<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style x:Key="logo" TargetType="Image">
        <Setter Property="Source" Value="/Resources/Images/logo.png" />
        <Setter Property="Width" Value="370" />
        <Setter Property="Height" Value="119" />
        <Setter Property="HorizontalAlignment" Value="Left" />
        <Setter Property="VerticalAlignment" Value="Top" />
        <Setter Property="Source" Value="/Resources/Images/logo.png" />
    </Style>

    <Style x:Key="logoLeft" TargetType="Image" BasedOn="{StaticResource logo}">
        <Setter Property="HorizontalAlignment" Value="Left" />
        <Setter Property="VerticalAlignment" Value="Top" />
        <Setter Property="Margin" Value="125.09 66.8 0 0" />
    </Style>

    <Style x:Key="logoRight" TargetType="Image" BasedOn="{StaticResource logo}">
        <Setter Property="HorizontalAlignment" Value="Right" />
        <Setter Property="VerticalAlignment" Value="Top" />
        <Setter Property="Margin" Value="0 66.8 125.09 0" />
    </Style>

    <Style x:Key="Image1" TargetType="Image">
        <Setter Property="Source" Value="/Resources/Images/bg.png" />
        <Setter Property="Width" Value="1326" />
        <Setter Property="Height" Value="1080" />
        <Setter Property="HorizontalAlignment" Value="Left" />
    </Style>

    <Style x:Key="who_are_we" TargetType="Image">
        <Setter Property="Source" Value="/Resources/Images/who_are_we.png" />
        <Setter Property="Width" Value="258.08" />
        <Setter Property="Height" Value="60.54" />
    </Style>

    <Style x:Key="what_are_we_doing" TargetType="Image">
        <Setter Property="Source" Value="/Resources/Images/what_are_we_doing.png" />
        <Setter Property="Width" Value="322.7" />
        <Setter Property="Height" Value="60" />
    </Style>

    <Style x:Key="backgroundAboutPage" TargetType="Image" >
        <Setter Property="Source" Value="/Resources/Images/BackgroundAbout.png" />
        <Setter Property="VerticalAlignment" Value="Bottom" />
    </Style>

    <Style x:Key="dataAbout" TargetType="Image">
        <Setter Property="Width" Value="1605" />
        <Setter Property="Height" Value="693" />
        <Setter Property="Margin" Value="0 39 0 0" />
    </Style>

    <Style x:Key="projectsBackground" TargetType="Image">
        <Setter Property="Source" Value="/Resources/Images/projectsBacground.png" />
        <Setter Property="Height" Value="702" />
        <Setter Property="Width" Value="2064" />
        <Setter Property="VerticalAlignment" Value="Bottom" />
    </Style>

    <Style x:Key="popupImage" TargetType="Image">
        <Setter Property="Height" Value="175" />
        <Setter Property="Width" Value="175" />
        <Setter Property="Margin" Value="0 64 0 0" />
    </Style>

    <Style x:Key="paymentMethodPopup" TargetType="Grid">
        <Setter Property="Background" Value="White" />
        <Setter Property="Height" Value="684" />
        <Setter Property="Width" Value="1755" />
    </Style> 

</ResourceDictionary>