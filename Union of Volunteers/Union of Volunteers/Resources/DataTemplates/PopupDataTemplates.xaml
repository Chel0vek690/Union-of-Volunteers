<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:popups="clr-namespace:Union_of_Volunteers.ViewModels.Popups"
                    xmlns:popups1="clr-namespace:Union_of_Volunteers.Views.Popups">
    <DataTemplate DataType="{x:Type popups:PasswordPopupViewModel}">
        <popups1:PasswordPopupView DataContext="{Binding}"/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type popups:PaymentMethodViewModel}">
        <popups1:PaymentMethodPopupView DataContext="{Binding}"/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type popups:CardMethodPopupViewModel}">
        <popups1:CardMethodPopupView DataContext="{Binding}"/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type popups:QrMethodPopupViewModel}">
        <popups1:QrMethodPopupView DataContext="{Binding}"/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type popups:DonationProcessingPopupViewModel}">
        <popups1:DonationProcessingPopupView DataContext="{Binding}"/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type popups:DonationSentSuccessfullyPopupViewModel}">
        <popups1:DonationSentSuccessfullyPopupView DataContext="{Binding}"/>
    </DataTemplate>
</ResourceDictionary>