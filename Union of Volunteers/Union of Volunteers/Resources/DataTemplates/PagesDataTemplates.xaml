<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:pages="clr-namespace:Union_of_Volunteers.ViewModels.Pages"
                    xmlns:pages1="clr-namespace:Union_of_Volunteers.Views.Pages">
    <DataTemplate DataType="{x:Type pages:MainPageViewModel}">
        <pages1:MainView DataContext="{Binding}"/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type pages:AboutPageViewModel}">
        <pages1:AboutView DataContext="{Binding}" />
    </DataTemplate>
</ResourceDictionary>